-- Database: ecards

-- DROP DATABASE IF EXISTS ecards;

CREATE DATABASE ecards
    WITH
    OWNER = margarida
    ENCODING = 'UTF8'
    LC_COLLATE = 'en_US.UTF-8'
    LC_CTYPE = 'en_US.UTF-8'
    LOCALE_PROVIDER = 'libc'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1
    IS_TEMPLATE = False;

COMMENT ON DATABASE ecards
    IS 'DIO - ecards';